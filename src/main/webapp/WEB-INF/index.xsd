<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="qualified"
        targetNamespace="http://www.geworkbench.org/service/index"
        xmlns:tns="http://www.geworkbench.org/service/index">

    <element name="indexRequest">
        <simpleType>
            <restriction base="string">
                <pattern value="([A-Z]|[a-z])+"/>
            </restriction>
        </simpleType>
    </element>

    <element name="indexResponse" type="string"/>
</schema>